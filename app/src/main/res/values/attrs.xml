<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--PicassoImageView-->
    <declare-styleable name="PicassoImageView">
        <attr name="placeholderImage" format="reference"></attr>
    </declare-styleable>
    <!--Minilayout-->
    <declare-styleable name="NativeMinilayout">
        <attr name="miniName" format="string"></attr>
        <attr name="miniLeftImge" format="reference"></attr>
        <attr name="showBottomView" format="boolean"></attr>
        <attr name="miniNumber" format="string"></attr>
    </declare-styleable>
    <!--CommonActionBar-->
    <declare-styleable name="CommonActionBar">
        <attr name="imageLeft" format="reference"></attr>
        <attr name="title" format="string"></attr>
        <attr name="imageRight" format="reference"></attr>
    </declare-styleable>
</resources>